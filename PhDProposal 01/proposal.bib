journal = {Science (New York, N.Y.)},
keywords = {Animals,Biodiversity,Competitive Behavior,Conservation of Natural Resources,Ecosystem,Evolution,Host-Parasite Interactions,Larva,Larva: parasitology,Larva: physiology,Longevity,Mathematics,Models, Biological,Parasites,Parasites: physiology,Population Dynamics,Probability},
month = oct,
