journal = {Nature},
keywords = {Acids,Acids: analysis,Antarctic Regions,Atmosphere,Atmosphere: chemistry,Calcium Carbonate,Calcium Carbonate: analysis,Calcium Carbonate: metabolism,Carbon,Carbon Isotopes,Carbon: analysis,Carbon: metabolism,Diatoms,Diatoms: metabolism,Geologic Sediments,Geologic Sediments: chemistry,History, Ancient,Hydrogen-Ion Concentration,Ice Cover,Models, Biological,Oceans and Seas,Phytoplankton,Phytoplankton: metabolism,Seawater,Seawater: analysis,Seawater: chemistry,Time Factors},
month = apr,
number = {7190},
